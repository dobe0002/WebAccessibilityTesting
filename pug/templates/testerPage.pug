extends mainTemplate.pug

block title
block headIncludes
	+headIncludes('../')

block append head
block breadCrumbs
	+breadcrumbSamplePage
		block title

block contentContainer
	.row
		.col
			h1
				block title
			.card
				.card-body
					.row
						.col-sm-2
							+fal('clipboard-list','fa-9x presentationRole')
						.col
							h2.h3 Things to check for:
							block checkForIntro
							ul.fa-ul
								block checkFor
							block example
							ul.list-inline.testerActionButtons
								li.list-inline-item
									button.btn.btn-outline-dark.btn-sm.toggle-warnings-btn
										|Toggle hints
								li.list-inline-item
									block howToCheckURL
	block content

block outsideLandmark
block bodyIncludes
	+bodyIncludes('../')
block extraCode